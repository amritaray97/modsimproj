{
  "model": {
    "type": "SEIR",
    "description": "SEIR model with stochastic noise"
  },
  "parameters": {
    "beta": 0.6,
    "sigma": 0.2,
    "gamma": 0.1,
    "population": 1.0
  },
  "initial_conditions": {
    "S0": 0.99,
    "E0": 0.0,
    "I0": 0.01,
    "R0": 0.0
  },
  "simulation": {
    "t_start": 0,
    "t_end": 300,
    "num_points": 1000,
    "method": "RK45"
  },
  "stochastic": {
    "enabled": true,
    "noise_level": 0.05,
    "num_realizations": 5,
    "seed": 42
  },
  "output": {
    "save_plots": true,
    "output_dir": "results",
    "plot_format": "png",
    "dpi": 300,
    "show_plots": false
  }
}
