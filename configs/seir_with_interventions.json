{
  "model": {
    "type": "SEIR",
    "description": "SEIR model with lockdown intervention"
  },
  "parameters": {
    "beta": 0.6,
    "sigma": 0.2,
    "gamma": 0.1,
    "population": 1.0
  },
  "initial_conditions": {
    "S0": 0.99,
    "E0": 0.0,
    "I0": 0.01,
    "R0": 0.0
  },
  "simulation": {
    "t_start": 0,
    "t_end": 300,
    "num_points": 1000,
    "method": "RK45"
  },
  "interventions": [
    {
      "name": "Early Lockdown",
      "start_time": 30,
      "duration": 30,
      "effectiveness": 0.7,
      "intervention_type": "reduction"
    }
  ],
  "output": {
    "save_plots": true,
    "output_dir": "results",
    "plot_format": "png",
    "dpi": 300,
    "show_plots": false
  }
}
