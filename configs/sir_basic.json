{
  "model": {
    "type": "SIR",
    "description": "Basic SIR model simulation"
  },
  "parameters": {
    "beta": 0.5,
    "gamma": 0.1,
    "population": 1.0
  },
  "initial_conditions": {
    "S0": 0.99,
    "I0": 0.01,
    "R0": 0.0
  },
  "simulation": {
    "t_start": 0,
    "t_end": 200,
    "num_points": 1000,
    "method": "RK45"
  },
  "output": {
    "save_plots": true,
    "output_dir": "results",
    "plot_format": "png",
    "dpi": 300,
    "show_plots": false
  }
}
